<div class='container '>
  <h1>My Tasks!</h1>
  <ul class= 'list-unstyled'>
    <% @tasks.each do |task| %>
    <li><%= link_to task.title, task_path(task), class:'text-decoration-none'%></li>
    <% end %>
  </ul>
  <p><%= link_to 'Create new task!', tasks_new_path, class:'text-decoration-none'%></p>
</div>
